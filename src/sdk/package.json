{
  "name": "@nirmiteeio/fhir-sdk",
  "version": "1.0.0",
  "description": "Universal FHIR SDK for SMART on FHIR integrations. Supports Epic, Cerner, Allscripts, Athena Health, and more. Framework-agnostic with React hooks included.",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./hooks": {
      "types": "./dist/hooks/index.d.ts",
      "import": "./dist/hooks/index.mjs",
      "require": "./dist/hooks/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "MODULARITY.md",
    "EXAMPLES.md"
  ],
  "scripts": {
    "build": "tsup",
    "build:watch": "tsup --watch",
    "prepublishOnly": "npm run build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest --testPathPattern=unit",
    "test:unit:core": "jest --testPathPattern=unit --testPathIgnorePatterns=hooks.test",
    "test:integration": "jest --testPathPattern=integration",
    "test:verbose": "jest --verbose",
    "test:ci": "jest --ci --coverage --maxWorkers=2"
  },
  "keywords": [
    "fhir",
    "smart-on-fhir",
    "ehr",
    "emr",
    "healthcare",
    "oauth2",
    "epic",
    "cerner",
    "allscripts",
    "athena",
    "nextgen",
    "meditech",
    "eclinicalworks",
    "patient-data",
    "health-records",
    "interoperability",
    "hl7",
    "fhir-r4",
    "healthcare-api",
    "medical-records"
  ],
  "author": "Nirmitee",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nirmiteeio/fhir-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/nirmiteeio/fhir-sdk/issues"
  },
  "homepage": "https://github.com/nirmiteeio/fhir-sdk#readme",
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "react": "^18.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    }
  },
  "dependencies": {
    "pkce-challenge": "^4.1.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^20",
    "@types/react": "^18",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "react": "^18",
    "ts-jest": "^29.4.5",
    "tsup": "^8.5.1",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
